{
  "name": "u9-plateau-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "prisma generate && tsc -p .",
    "start": "node dist/server.js",
    "prisma:push": "prisma db push",
    "prisma:push:accept-loss": "prisma db push --accept-data-loss",
    "prisma:studio": "prisma studio",
    "migrate:sqlite-to-postgres": "node scripts/migrate-sqlite-to-postgres.mjs",
    "backfill:user-ownership": "node scripts/backfill-user-ownership.mjs"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.7.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nanoid": "^5.0.7",
    "nodemailer": "^6.9.13",
    "pg": "^8.13.3",
    "qrcode": "^1.5.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.6",
    "@types/node": "^22.5.1",
    "@types/nodemailer": "^7.0.1",
    "@types/qrcode": "^1.5.5",
    "prisma": "^5.19.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  }
